// prime or not
// by gilu hehe prac lang

#include <stdio.h>

int main() {
    int num, i, checker=0;
    
    printf("Enter number: ");
    scanf("%d", &num);
    
    for (i=2;i<=num/2;i++)
    {
        if (num%i==0)
        {
            checker = 1;
            break;
        }
    }
    
    if (checker==0)
    printf("Prime.");
    else
    printf("Not Prime.");
    
    
  return 0;
}
